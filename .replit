modules = ["python-3.11"]
[agent]
integrations = ["python_openai:1.0.0", "python_gemini:1.0.0"]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["freetype", "glibcLocales", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "tcl", "tk", "zlib"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "TrueCraft App"

[[workflows.workflow]]
name = "TrueCraft App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "uv run streamlit run TrueCraft.py --server.port 5000 --server.address 0.0.0.0"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

